                <materialDesign:Card Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="3" Background="{DynamicResource SecondaryAccentBrush}"
    materialDesign:ShadowAssist.ShadowDepth="Depth2"
    Padding="3" Margin="5" VerticalAlignment="Stretch" FontWeight="Bold">
