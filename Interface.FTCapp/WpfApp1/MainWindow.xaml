                <Grid.RowDefinitions>
                    <RowDefinition Height="50"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="50"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="5*"/>
                    <ColumnDefinition Width="2"/>
                    <ColumnDefinition Width="5*"/>
                    <ColumnDefinition Width="2"/>
                    <ColumnDefinition Width="5*"/>
                </Grid.ColumnDefinitions>
                <!--CABECALHO  FLUXO CASO TESTE-->
                <materialDesign:Card Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="5"  Background="{DynamicResource SecondaryAccentBrush}"
    materialDesign:ShadowAssist.ShadowDepth="Depth2"
    Padding="3" Margin="5" VerticalAlignment="Stretch"  HorizontalContentAlignment="Center">
                    <TextBlock Grid.ColumnSpan="3" Foreground="{DynamicResource SecondaryAccentForegroundBrush}"  HorizontalAlignment="Center" TextWrapping="Wrap" Text="FLUXO CASO TESTE" VerticalAlignment="Center" FontWeight="Bold" />
