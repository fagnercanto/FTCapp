                                <DataGridTemplateColumn.CellTemplate >
                                    <DataTemplate >

                                        <TextBox  VerticalAlignment="Center" />
                                        
                                        
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>

                            <DataGridTemplateColumn  Header="CDGESTAO">
                                    <DataGridTemplateColumn.CellTemplate >
                                        <DataTemplate >

                                            <TextBox  VerticalAlignment="Center" />


                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                            <DataGridTemplateColumn  Header="NUEDITALLICITACAO">
                                <DataGridTemplateColumn.CellTemplate >
                                    <DataTemplate >

                                        <TextBox  VerticalAlignment="Center" />


                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn  Header="ANOEDITALLICITACAO">-->
                            <!--<DataGridTemplateColumn.CellTemplate >
                                    <DataTemplate >

                                        <TextBox  VerticalAlignment="Center" />


                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>-->
                            <DataGridTemplateColumn Header="ACAO">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Horizontal">
                                            <Button HorizontalAlignment="Right" BorderBrush="#FFF5F9FF"
                                              Style="{StaticResource MaterialDesignFloatingActionMiniAccentButton}"
                                              ToolTip="MaterialDesignFloatingActionMiniAccentButton" Height="18" Width="18" Margin="2" Background="#FF616161" Foreground="#FFEAFAF1">
                                                <materialDesign:PackIcon
                                                Kind="Plus"
                                                Height="15"
                                                Width="15" 
                                                    FontWeight="Bold"
                                                     
                                                    />
                                            </Button>
                                            <Button HorizontalAlignment="Right" BorderBrush="#FFF5F9FF"
                                              Style="{StaticResource MaterialDesignFloatingActionMiniAccentButton}"
                                              ToolTip="Rodar Teste" 
                                                Height="18" 
                                                Width="18" 
                                                Margin="2" 
                                                Background="#FF616161" 
                                                Foreground="#FFEAFAF1">
                                                <materialDesign:PackIcon
                                                Kind="Play"
                                                Height="15"
                                                Width="15" 
                                                    FontWeight="Bold"
                                                     
                                                    />
                                            </Button>
                                        </StackPanel>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
                <!--GID PANEL <GRID>-->
                <!-- GID PANEL BORDER -->
                <Border Grid.Column="1"
                        Grid.Row="1"
                     Background="{DynamicResource MaterialDesignDivider}"
                     Width="1"
                     VerticalAlignment="Stretch"
                     SnapsToDevicePixels="True" />
                <!--BORDER-->
                <!--GID PANEL RODAPEGRID-->
                <materialDesign:Card Grid.Column="2" Grid.Row="2" Background="{DynamicResource SecondaryAccentBrush}"
                        materialDesign:ShadowAssist.ShadowDepth="Depth2"
                        Padding="3" Margin="5" VerticalAlignment="Stretch"  HorizontalContentAlignment="Center">
                    <Button
                        Style="{StaticResource MaterialDesignRaisedDarkButton}"
                                        
                        ToolTip="Resource name: MaterialDesignRaisedLightButton" 
                        HorizontalAlignment="Right"  VerticalAlignment="Center" FontWeight="Bold" Margin="0,0,5,0" Height="20" FontSize="10" FontFamily="Segoe UI">
                        RODAR CASOS TESTES
                    </Button>
                </materialDesign:Card>
                <!--GID PANEL RODAPEGRID-->
                <!--GID PANEL RODAPEGRID-->
                <materialDesign:Card Grid.Column="2" Grid.Row="2" Background="{DynamicResource SecondaryAccentBrush}"
                        materialDesign:ShadowAssist.ShadowDepth="Depth2"
                        Padding="3" Margin="5" VerticalAlignment="Stretch"  HorizontalContentAlignment="Center">
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                        <Button
                        Style="{StaticResource MaterialDesignRaisedDarkButton}"
                                        
                        ToolTip="Resource name: MaterialDesignRaisedLightButton" 
                        HorizontalAlignment="Right"  VerticalAlignment="Center" FontWeight="Bold" Margin="0,0,5,0" Height="20" FontSize="10" FontFamily="Segoe UI">
                            NOVA LINHA
                        </Button>
                        <Button
                        Style="{StaticResource MaterialDesignRaisedDarkButton}"
                                        
                        ToolTip="Resource name: MaterialDesignRaisedLightButton" 
                        HorizontalAlignment="Right"  VerticalAlignment="Center" FontWeight="Bold" Margin="0,0,5,0" Height="20" FontSize="10" FontFamily="Segoe UI">
                            RODAR CASOS TESTES
                        </Button>
                    </StackPanel>
                </materialDesign:Card>
                <!--GID PANEL RODAPE GRID-->
                <!--GID PANEL SCROPT SQL-->
                <Grid Grid.Column="0" Grid.Row="1" Margin="5,0,5,5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="20"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <TextBlock Grid.Row="0" Margin="5,0,0,5">SCRIPT SQL</TextBlock>
                    <DockPanel Grid.Row="1" >
                        <ToolBar DockPanel.Dock="Top">
                            <Button Command="ApplicationCommands.Open"  BorderBrush="#FFF5F9FF"
                                              Style="{StaticResource MaterialDesignFloatingActionMiniAccentButton}"
                                               Height="24" Width="24" Margin="2" 
                                    Background="#FF424242" Foreground="#FFEAFAF1">
                                <materialDesign:PackIcon
                                                Kind="Folder"
                                                Height="12"
                                                Width="12" 
                                                    FontWeight="Bold"
                                                     
                                                    />
                            </Button>


                            <Button Command="ApplicationCommands.Save" BorderBrush="#FFF5F9FF"
                                              Style="{StaticResource MaterialDesignFloatingActionMiniAccentButton}"
                                               Height="24" Width="24" Margin="2" 
                                    Background="#FF424242" Foreground="#FFEAFAF1">
                                <materialDesign:PackIcon
                                                Kind="ContentSave"
                                                Height="12"
                                                Width="12" 
                                                    FontWeight="Bold"
                                                     
                                                    />
                            </Button>
                            <Separator />
                            <ToggleButton Command="EditingCommands.ToggleBold" Name="btnBold" BorderBrush="#FFF5F9FF"
                                             Style="{StaticResource MaterialDesignActionAccentToggleButton}"
                                               Height="24" Width="24" Margin="2" 
                                    Background="#FF424242" Foreground="#FFEAFAF1">
                                <materialDesign:PackIcon
                                                Kind="FormatBold"
                                                Height="12"
                                                Width="12" 
                                                    FontWeight="Bold"
                                                     
                                                    />
                            </ToggleButton>
                            <ToggleButton Command="EditingCommands.ToggleItalic" Name="btnItalic" BorderBrush="#FFF5F9FF"
                                              Style="{StaticResource MaterialDesignActionAccentToggleButton}"
                                               Height="24" Width="24" Margin="2" 
                                    Background="#FF424242" Foreground="#FFEAFAF1">
                                <materialDesign:PackIcon
                                                Kind="FormatItalic"
                                                Height="12"
                                                Width="12" 
                                                    FontWeight="Bold"
                                                     
                                                    />
                            </ToggleButton>
                            <ToggleButton Command="EditingCommands.ToggleUnderline" Name="btnUnderline" BorderBrush="#FFF5F9FF"
                                               Style="{StaticResource MaterialDesignActionAccentToggleButton}"
                                               Height="24" Width="24" Margin="2" 
                                    Background="#FF424242" Foreground="#FFEAFAF1">
                                <materialDesign:PackIcon
                                                Kind="FormatUnderline"
                                                Height="12"
                                                Width="12" 
                                                    FontWeight="Bold"
                                                     
                                                    />
                            </ToggleButton>
                            <ComboBox SelectedIndex="0" Name="cmbFontFamily"  SelectionChanged="cmbFontFamily_SelectionChanged" />
                            <ComboBox SelectedIndex="0" Name="cmbFontSize"  IsEditable="True" TextBoxBase.TextChanged="cmbFontSize_TextChanged" />
                        </ToolBar>
                        <RichTextBox Name="rtbEditor" SelectionChanged="rtbEditor_SelectionChanged" >

                        </RichTextBox>
                    </DockPanel>



                </Grid>
                <!--SCROPT SQL-->
                <!--RODAPE SQL-->
                <materialDesign:Card Grid.Column="0" Grid.Row="2" Background="{DynamicResource SecondaryAccentBrush}"
                        materialDesign:ShadowAssist.ShadowDepth="Depth2"
                        Padding="3" Margin="5" VerticalAlignment="Stretch"  HorizontalContentAlignment="Center">
                    <Button
                        Style="{StaticResource MaterialDesignRaisedDarkButton}"
                                        
                        ToolTip="Resource name: MaterialDesignRaisedLightButton" 
                        HorizontalAlignment="Right"  VerticalAlignment="Center" FontWeight="Bold" Margin="0,0,5,0" Height="20" FontSize="10" FontFamily="Segoe UI">
                        MONTAR GRID
                    </Button>
                </materialDesign:Card>

                <!--RODAPE SQL-->
            </Grid>
        </materialDesign:Card>

        <!-- FIM GRID ELEMENTOS-->


        <!--FLUXO CASO TESTE -->

