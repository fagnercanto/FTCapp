                </materialDesign:Card>
                <!--CABECALHO GID PANEL-->
                <!--<GRID> GID PANEL-->
                <Grid Grid.Row="1" Grid.Column="2" 
                       
                      Background="#00000000"
                      HorizontalAlignment="Stretch" 
                      VerticalAlignment="Stretch"
                      Margin="5,0,5,5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="20"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <TextBlock Grid.Row="0" Margin="5,0,0,5">GRID</TextBlock>

                    <DataGrid Name="GridElementos" Grid.Row="1" Margin="0" FontSize="9" AutoGenerateColumns="False"  >
                        <DataGrid.Columns>
                            <DataGridTextColumn Binding="{Binding CDUNIDADEGESTORA}" Header="CDUNIDADEGESTORA"/>
                            <DataGridTextColumn Binding="{Binding CDGESTAO}" Header="CDGESTAO"/>
                            <DataGridTextColumn  Binding="{Binding NUEDITALLICITACAO}" Header="NUEDITALLICITACAO"/>
                            <DataGridTextColumn Binding="{Binding ANOEDITALLICITACAO}" Header="ANOEDITALLICITACAO"/>
                            <DataGridTextColumn Binding="{Binding vNUMERODO }" Header=" vNUMERODO "/>
                            <DataGridTextColumn Binding="{Binding vNUMERODO2 }" Header=" vNUMERODO2 "/>
                            <DataGridTextColumn Binding="{Binding vNUMERODO3 }" Header=" vNUMERODO3 "/>
                            <DataGridTextColumn Binding="{Binding vNUMERODO4 }" Header=" vNUMERODO4 "/>
                            <DataGridTextColumn Binding="{Binding vCDUNIDADEGESTORA }" Header=" vCDUNIDADEGESTORA "/>
                            <DataGridTextColumn Binding="{Binding vCDUNIDADEGESTORA2 }" Header=" vCDUNIDADEGESTORA2 "/>
                            <DataGridTextColumn Binding="{Binding vDTNOVASITUACAO }" Header=" vDTNOVASITUACAO "/>
                            <DataGridTextColumn Binding="{Binding vDTNOVASITUACAO2 }" Header=" vDTNOVASITUACAO2 "/>
                            <DataGridTextColumn Binding="{Binding vNOVASITUACAO }" Header=" vNOVASITUACAO"/>
                            <DataGridTextColumn Binding="{Binding vNUMERODOREPUBLICADO }" Header=" vNUMERODOREPUBLICADO"/>
                            <DataGridTextColumn Binding="{Binding vSITUACAOATUAL }" Header=" vSITUACAOATUAL"/>

