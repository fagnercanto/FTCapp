<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="FCTapp" WindowState="Maximized" ShowInTaskbar="False" 
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        TextElement.FontWeight="Regular"
        TextElement.FontSize="13"
        TextOptions.TextFormattingMode="Ideal" 
        TextOptions.TextRenderingMode="Auto"        
        Background="{DynamicResource MaterialDesignPaper}"
        FontFamily="{DynamicResource MaterialDesignFont}">
    <Window.CommandBindings>
        <CommandBinding Command="ApplicationCommands.Open" Executed="Open_Executed" />
        <CommandBinding Command="ApplicationCommands.Save" Executed="Save_Executed" />
    </Window.CommandBindings>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="120"/>
            <!--DADOS CONCEITO-->
            <RowDefinition Height="1*"/>
            <!--GRID ELEMENTOS-->
            <RowDefinition Height="3*"/>
            <!--FLUXO CASO TESTE-->
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="30*"/>
            <ColumnDefinition Width="10*"/>
            <!--TESTES COMPARTILHADOS-->
        </Grid.ColumnDefinitions>

        <!--DADOS CONCEITO-->

        <materialDesign:Card Grid.Row="0" Grid.Column="0"
    materialDesign:ShadowAssist.ShadowDepth="Depth1"
    Padding="5" VerticalAlignment="Stretch" Margin="10,10,5,5">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="40"/>
                    <RowDefinition Height="*"/>

                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <materialDesign:Card Grid.Column="0" Grid.Row="0"
    materialDesign:ShadowAssist.ShadowDepth="Depth2"
    Padding="3" Margin="5" VerticalAlignment="Stretch" Background="{DynamicResource SecondaryAccentBrush}">
                    <TextBlock Foreground="{DynamicResource SecondaryAccentForegroundBrush}" HorizontalAlignment="Center" TextWrapping="Wrap" Text="DADOS CONCEITO" VerticalAlignment="Center" FontWeight="Bold"/>
                </materialDesign:Card>
                <Grid Grid.Column="0" 
                      Grid.Row="2" 
                      Background="#00000000"
                      HorizontalAlignment="Stretch" 
                      VerticalAlignment="Stretch"
                      Margin="5,0,5,5">
                    <StackPanel Orientation="Horizontal" Margin="5,0">
                        <Label  Content="CLIENTE:" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        <ComboBox >
                            <ComboBoxItem Content="RN"/>
                            <ComboBoxItem Content="SC"/>
                            <ComboBoxItem IsSelected="True" Content="SALVADOR"/>

                        </ComboBox>
                        <Label  Content="AMBIENTE:" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        <ComboBox >
                            <ComboBoxItem IsSelected="True" Content="DSV"/>
                            <ComboBoxItem Content="HOM"/>
                            <ComboBoxItem Content="PDR"/>

                        </ComboBox>
                        <Label  Content="MODULO:" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        <ComboBox >
                            <ComboBoxItem Content="PPA" />
                            <ComboBoxItem Content="AVI"/>
                            <ComboBoxItem Content="COB"/>
                            <ComboBoxItem Content="CTB"/>
                            <ComboBoxItem Content="DIV"/>
                            <ComboBoxItem Content="PGF"/>
                            <ComboBoxItem Content="ORC"/>
                            <ComboBoxItem Content="LDO"/>
                            <ComboBoxItem Content="LOA"/>
                            <ComboBoxItem Content="OST"/>
                            <ComboBoxItem Content="RPG"/>
                            <ComboBoxItem Content="DEM"/>
                            <ComboBoxItem IsSelected="True" Content="CTR"/>
                            <ComboBoxItem Content="FIN"/>
                            <ComboBoxItem Content="SAT"/>
                            <ComboBoxItem Content="TCM"/>
                            <ComboBoxItem Content="ADM"/>
                            <ComboBoxItem Content="BIB"/>
                            <ComboBoxItem Content="SEG"/>
                            <ComboBoxItem Content="SER"/>
                            <ComboBoxItem Content="CAD"/>
                            <ComboBoxItem Content="CCO"/>
                            <ComboBoxItem Content="TCE"/>
                        </ComboBox>
                        <Label Content="TRANSAÇÃO:" VerticalAlignment="Center"/>
                        <ComboBox  IsDropDownOpen="False" >
                            <ComboBoxItem Content="Listar Ordenadores Unidade Gestora"  />
                            <ComboBoxItem Content="Listar Ordenadores Unidade Gestora - Geral"  />
                            <ComboBoxItem Content="Listar Tipo Ato Legal"  />
                            <ComboBoxItem Content="Manter Complemento"  />
                            <ComboBoxItem Content="Manter Feriado"  />
                            <ComboBoxItem Content="Manter Histórico Padrão"  />
                            <ComboBoxItem Margin="5,0" IsSelected="True" Content="Listar Modalidade Licitação" />
                            <ComboBoxItem Content="Manter Modalidade Licitação"  />
                            <ComboBoxItem Content="Manter Tipo Ato Legal"  />
                            <ComboBoxItem Content="Nomear Ordenador"  />
                            <ComboBoxItem Content="Alterar Histórico Nota Empenho"  />
                            <ComboBoxItem Content="Definir Data Entrega Nota Empenho"  />
                            <ComboBoxItem Content="Imprimir Nota Empenho"  />
                            <ComboBoxItem Content="Listar Descrição Nota Empenho"  />
                            <ComboBoxItem Content="Listar Nota Empenho"  />
                            <ComboBoxItem Content="Listar Nota Empenho Célula"  />
                            <ComboBoxItem Content="Listar Nota Empenho Credor"  />
                            <ComboBoxItem Content="Listar Nota Empenho Credor - Geral"  />
                        </ComboBox>
                        <Label  Content="USUARIO:" VerticalAlignment="Center"/>
